# Environment variables for cloud deployment
# Copy this file to .env and fill in the actual values

# Database password for PostgreSQL
DB_PASSWORD=your_secure_database_password_here

# API key for the FastAPI service
API_KEY=your_api_key_here

# Optional: Database host (defaults to 'db' in docker-compose)
# DB_HOST=db

# Optional: Database port (defaults to 5432)
# DB_PORT=5432

# Optional: Database name (defaults to 'driving_schools')
# DB_NAME=driving_schools

# Optional: Database user (defaults to 'scraper_user')
# DB_USER=scraper_user
